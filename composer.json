{
    "name": "klaussilveira/neo4j-ogm-bundle",
    "description": "This bundle provides a simple integration of the Neo4j PHP Object Graph Mapper into Symfony2",
    "type": "symfony-bundle",
    "keywords": ["neo4j", "graph", "database", "ogm"],
    "homepage": "https://github.com/klaussilveira/neo4j-ogm-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Klaus Silveira",
            "email": "klaussilveira@php.net"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "hirevoice/neo4jphp-ogm": "dev-master"
    },
    "target-dir": "Neo4j/OGM/OGMBundle",
    "autoload": {
        "psr-0": {
            "Neo4j\\OGM\\OGMBundle": ""
        }
    }
}